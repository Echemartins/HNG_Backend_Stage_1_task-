{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*.ts"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "API_KEY": "@api_key"
    },
    "ignore": [
      "node_modules",
      "dist",
      "test"
    ]
  }
  